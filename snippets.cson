#	Source tags
  'Link tag':
    'prefix': 'link'
    'body': """
		<link rel="stylesheet" href="${1:css/master.css}">$2
	"""
  'Style tag':
    'prefix': 'style'
    'body': """
		<style>
			$1
		</style>$2
	"""
  'Script tag':
    'prefix': 'script'
    'body': """
		<script${1: src="${2:js/script.js}"}>$3</script>$4
	"""
#	Content tags
  'Div tag':
    'prefix': 'div'
    'body': """
		<div ${1:class="$2"}>
			$3
		</div>$4
	"""
#	FontAwesome
  'FontAwesome Chevrons':
    'prefix': 'fachevrons'
    'body': """
		<i class="fas fa-chevron-${1:(left,right)}"></i>$3
	"""


