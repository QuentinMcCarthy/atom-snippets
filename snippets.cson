# JS
'.source.js':
#	Overrides
  'JS Function':
    'prefix': 'func'
    'body': """
		function $1($2){
			$3
		}$4
	"""
  'JS for loop':
    'prefix': 'for'
    'body': """
		for(var i = 0; i < ${1:Array}.length; i++){
			$2
		}$3
	"""
  'JS switch':
    'prefix': 'switch'
    'body': """
		switch(${1:expression}){
			case ${2:expression}:
				$3
				break;
			default:
				$4
		}$5
	"""
  'JS try':
    'prefix': 'try'
    'body': """
		try {
			$1
		}
		catch(err){
			$2
		}$3
	"""
  'JS if':
    'prefix': 'if'
    'body': """
		if(${1:true}){
			$2
		}$3
	"""
  'JS if else':
    'prefix': 'ife'
    'body': """
		if(${1:true}){
			$2
		} else{
			$3
		}$4
	"""
  'JS iife':
    'prefix': 'iife'
    'body': """
		(function(){
			$1
		}());$2
	"""
  'JS Require':
    'prefix': 'req'
    'body': """
		const $1 = require("$1");
	"""
#	Events
  'jQuery Click Event':
    'prefix': 'jqclick'
    'body': """
		$("$1").click(function(){
			$2
		});$3
	"""
  'jQuery Ready Event':
    'prefix': 'jqready'
    'body': """
		$(${1:document}).ready(function(){
			$2
		});$3
	"""
  'jQuery Event Syntax':
    'prefix': 'jqevent'
    'body': """
		$($1).${2:event}(function(){
			$3
		});$4
	"""
#	Functions
  'JS XHTTP':
    'prefix': 'xhttp'
    'body': """
		var ${1:xhttp} = new XMLHttpRequest();
		${1:xhttp}.onreadystatechange = function(){
			${2:// xhttp.readyState
			// 	0- "Request not initialised",
			// 	1- "Server connection established",
			// 	2- "Request recieved",
			// 	3- "Processing the request",
			// 	4- "Request finished, response ready"
			// xhttp.status (https://en.wikipedia.org/wiki/list_of_HTTP_status_codes)
			// 	200- OK
			// 	403- Forbidden / need login
			// 	404- Not found}

			if(this.status == 403){
				${3:console.log("XHTTP Request: 403 - Forbidden");}
				return;
			}else if(this.status == 404){
				${4:console.log("XHTTP Request: 404 - Not Found");}
				return;
			}

			if(this.status == 200 && this.readyState == 4){
				${5:console.log("XHTTP Request: 200 - OK");
				console.log(this.responseText);}
			}
		}
		${1:xhttp}.open("${6:GET}", "$7", ${8:true});
		${1:xhttp}.send($9);$10
	"""
  'jQuery getJSON':
    'prefix': 'jqjson'
    'body': """
		$.getJSON("$1", function(data){
			$2
		}).done(function(data){
			$3
		});$4
	"""
  'jQuery Ajax':
    'prefix': 'jqajax'
    'body': """
		$.ajax({
			type: "${1:GET}",
			url: "$2",
			dataType: "${3:json}",
			success: function(${4:data}){
				${5:console.log(${4:data});}$6
			},
			error: function(err){
				${7:console.log("Error "+err.status);
				console.log(err);}
			}
		});
	"""
#	Methods
  'JS sort':
    'prefix': 'sort'
    'body': """
		${1:Array}.sort(${2:function(a,b){
			return ${3:a-b}
		}});$4
	"""
  'JS forEach':
    'prefix': 'foreach'
    'body': """
		${1:Array}.forEach(function(currentValue,index){
			$2
		});$3
	"""
#	Other
  'JS random number':
    'prefix': 'randint'
    'body': """
		${1:var ${2:randInt} = }Math.floor(${3:0} + (Math.random() * ${4:100}));$5
	"""
